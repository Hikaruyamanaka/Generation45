import java.util.Scanner;

public class Exercicio3 {

	public static void main(String[] args) {
		
		double tempo, hora, minutos, segundos, resto;
		
		Scanner leia = new Scanner(System.in);
		tempo = leia.nextInt();
		
		hora = tempo / 3600;
		resto = tempo % 3600;
		minutos = resto / 60;
		segundos = resto / 60;
		
			}

}
